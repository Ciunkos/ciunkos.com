{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": false,
        "targets": {
          "esmodules": true,
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ]
}
