{
  "name": "ciunkos.com",
  "version": "1.0.0",
  "description": "Personal blog and resume",
  "repository": {
    "type": "git",
    "url": "https://github.com/ciunkos/ciunkos-com.git"
  },
  "scripts": {
    "start":
      "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build": "npm run build:to \"build\"",
    "build:to":
      "webpack --config webpack.config.js --colors --env.production --env.path",
    "deploy":
      "webpack --config webpack.config.js --progress --colors --env.production --env.path \"D:\\www\\ciunkos.com\"",
    "start:server": "node build/server.js",
    "switch-locale": "node switch-locale",
    "switch-locale:en": "node switch-locale en",
    "switch-locale:pl": "node switch-locale pl",
    "test": "jest \\.test\\.js$",
    "test:snapshot": "jest \\.snapshot\\.js$ --ci -u",
    "test:snapshot:extract": "node jest/moveSnapshots.js",
    "test:visual":
      "node circleci-visual-regression-test && reg-cli /tmp/visual-regression/after /tmp/visual-regression/before /tmp/visual-regression/diff -R /tmp/visual-regression/index.html --ignoreChange",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.js\"",
    "prettier:debug": "prettier \"src/**/*.js\" --debug-check",
    "update": "ncu -x react-router -x history --upgradeAll -a",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "screenshot":
      "storybook-chrome-screenshot -p 9001 -c .storybook --silent -o /tmp/visual-regression/after"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "private": true,
  "devDependencies": {
    "@storybook/addon-storyshots": "3.3.3",
    "@storybook/addons": "3.3.3",
    "@storybook/react": "3.3.3",
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "circular-dependency-plugin": "^4.3.0",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^22.0.4",
    "jest-webpack-resolver": "^0.1.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "precss": "^2.0.0",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "reg-cli": "^0.10.9",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.6",
    "storybook-addon-specifications": "^2.1.1",
    "storybook-chrome-screenshot": "github:ciunkos/storybook-chrome-screenshot",
    "string-loader": "0.0.1",
    "style-loader": "^0.19.1",
    "transfer-webpack-plugin": "^0.1.4",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-stats-plugin": "^0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "dsv-loader": "^2.0.0",
    "express": "^4.16.2",
    "highlight.js": "^9.12.0",
    "history": "^3.2.1",
    "image-size-loader": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "nodemailer": "^4.4.1",
    "react": "^16.2.0",
    "react-body-classname": "^1.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.0",
    "react-helmet": "^5.2.0",
    "react-lowlight": "^1.1.1",
    "react-markdown": "^3.1.4",
    "react-router": "^3.0.5",
    "react-router-scroll": "^0.4.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTest.js",
    "modulePaths": ["<rootDir>/src/"],
    "moduleFileExtensions": ["js", "jsx"],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|snapshot).js?(x)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|txt|md)$":
        "<rootDir>/jest/fileTransformer.js",
      "\\.(json)$": "<rootDir>/jest/jsonLoader.js",
      "\\.(txt|md)$": "<rootDir>/jest/rawLoader.js"
    },
    "resolver": "jest-webpack-resolver",
    "automock": false
  },
  "jestWebpackResolver": {
    "webpackConfig": "webpack-dev-server.config.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "last 2 iOS versions"
  ],
  "locale": "en"
}
